generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PROD_DB_URL_POOLED")
  directUrl = env("PROD_DB_URL")
}

model Vehicle {
  id String @id
  name String
}
